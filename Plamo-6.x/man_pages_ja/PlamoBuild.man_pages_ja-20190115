#!/bin/sh
##############################################################
pkgbase='man_pages_ja'
vers='20190115'
url="https://linuxjm.osdn.jp/man-pages-ja-${vers}.tar.gz"
verify=""
arch=noarch
build=T1
src="man-pages-ja-${vers}"
OPT_CONFIG=''
DOCS='INSTALL README'
patchfiles=''
compress=txz
##############################################################
# TODO: インストールするマニュアルの選択は仮なのでパッケージが揃った辺りで見直す必要あり

source /usr/share/plamobuild_functions.sh

# このスクリプトで使う1文字変数の意味
#
# $W : このスクリプトを動かすカレントディレクトリ
# $S : ソースコードのあるディレクトリ(デフォルト: $W/${src})
# $B : ビルド用ディレクトリ(デフォルト: /tmp/build{,32})
# $P : ビルドしたファイルをインストールするディレクトリ（デフォルト: $W/work)


if [ $# -eq 0 ] ; then
  opt_download=0 ; opt_config=1 ; opt_build=1 ; opt_package=1
else
  opt_download=0 ; opt_config=0 ; opt_build=0 ; opt_package=0
  for i in $@ ; do
    case $i in
    download) opt_download=1 ;;
    config) opt_config=1 ;;
    build) opt_build=1 ;;
    package) opt_package=1 ;;
    esac
  done
fi
if [ $opt_download -eq 1 ] ; then
  download_sources
fi

if [ $opt_config -eq 1 ] ; then
  if [ -d $B ] ; then rm -rf $B ; fi ; cp -a $S $B
######################################################################
#  copy sources into build directory, patch and make in the build dir
######################################################################
  cd $B
  for patch in $patchfiles ; do
    patch -p1 < $W/$patch
  done
fi
    
if [ $opt_build -eq 1 ] ; then
  echo "Nothing to do."
fi

if [ $opt_package -eq 1 ] ; then
  check_root
  if [ -d $P ] ; then rm -rf $P ; fi ; mkdir -p $P
  cd $B
  install -v -d 755 -o root -g root $P/usr/share/man/ja_JP.UTF-8/man{1,2,3,4,5,6,7,8}
  install -v -o root -g root -m 644 manual/acl/man5/acl.5 $P/usr/share/man/ja_JP.UTF-8/man5
  install -v -o root -g root -m 644 manual/GNU_groff/man1/addftinfo.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/GNU_binutils/man1/addr2line.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/shadow/man8/adduser.8 $P/usr/share/man/ja_JP.UTF-8/man8
  install -v -o root -g root -m 644 manual/GNU_groff/man1/afmtodit.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/util-linux/man8/agetty.8 $P/usr/share/man/ja_JP.UTF-8/man8
  install -v -o root -g root -m 644 manual/apmd/man1/apm.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/apmd/man8/apmd.8 $P/usr/share/man/ja_JP.UTF-8/man8
  install -v -o root -g root -m 644 manual/apmd/man1/apmsleep.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/GNU_binutils/man1/ar.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/GNU_coreutils/man1/arch.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/net-tools/man8/arp.8 $P/usr/share/man/ja_JP.UTF-8/man8
  install -v -o root -g root -m 644 manual/GNU_binutils/man1/as.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/at/man5/at.allow.5 $P/usr/share/man/ja_JP.UTF-8/man5
  install -v -o root -g root -m 644 manual/at/man1/at.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/at/man8/atd.8 $P/usr/share/man/ja_JP.UTF-8/man8
  install -v -o root -g root -m 644 manual/at/man1/atq.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/at/man1/atrm.1 $P/usr/share/man/ja_JP.UTF-8/man1
  install -v -o root -g root -m 644 manual/at/man8/atrun.8 $P/usr/share/man/ja_JP.UTF-8/man8
